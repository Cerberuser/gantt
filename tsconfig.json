{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "declaration": true,
        "sourceMap": true
    }
}
